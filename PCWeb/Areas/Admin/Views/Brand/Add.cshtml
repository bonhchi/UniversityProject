@model PCWeb.Models.Source.Brand
@{
    ViewData["Title"] = "Thêm thương hiệu";
}
<h4 class="text-center">Thêm thương hiệu</h4>
<hr />
<div class="row">
    <div class="col-lg-10">
        <form asp-action="Add" asp-controller="Brand" method="post" enctype="multipart/form-data">
            <div class="form-group row">
                <label asp-for="BrandName" class="col-sm-3 col-form-label"></label>
                <div class="col-sm-6">
                    <input asp-for="BrandName" class="form-control" />
                </div>
                <span asp-validation-for="BrandName" class="text-danger"></span>
            </div>
            <div class="form-group row col-sm-3">
                <input type="submit" value="Thêm" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

