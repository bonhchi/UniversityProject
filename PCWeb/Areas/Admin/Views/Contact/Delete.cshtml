@model PCWeb.Models.Contact
@{
    ViewData["Title"] = "Xóa góp ý";
}
<h3>Bạn có chắc chắn xóa góp ý này không?</h3>
<br />
<div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="ContactId" />
        <input type="submit" value="Xóa" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-info">Quay về danh sách</a>
    </form>
</div>

